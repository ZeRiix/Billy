{% extends 'connected.html.twig' %}

{% block title %}Créer un devis{% endblock %}

{% block body %}
	<h1 class="mb-14 text-3xl font-semibold dark:text-dark-white">Création d'un devis</h1>
	<div class="p-14 rounded-xl bg-white dark:bg-dark-light-black">
		{{ form_start(form) }}
		<div class="flex flex-col gap-10 items-start">
			<div class="flex gap-5">
				{{ form_row(form.name) }}
			</div>
			<div class="w-full flex justify-end">
				{{ form_widget(form.submit) }}
			</div>		
		</div>
		{{ form_end(form) }}
	</div>
{% endblock %}