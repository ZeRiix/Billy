{% extends 'connected.html.twig' %}

{% block title %}Ajouter un utilisateur{% endblock %}

{% block body %}

<h1 class="mb-14 text-3xl font-semibold dark:dark:text-dark-white">Ajouter un utilisateur</h1>
<div class="p-14 rounded-xl bg-white dark:bg-dark-light-black">
	{{ form_start(form) }}
	<div class="flex flex-col gap-10 items-start">
		{{ form_row(form.email) }}
		<div class="w-full flex justify-end">
			{{ form_widget(form.submit) }}
		</div>
	</div>
</div>
{{ form_end(form) }}
{% endblock %}